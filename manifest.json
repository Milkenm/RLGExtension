{
  "manifest_version": 3,
  "name": "RLG Extension",
  "version": "1.1",
  "author": "Milkenm",
  "description": "Bump all trades on RL Garage with a single click.",
  "content_scripts": [
	// Global style
	{
	  "matches": [
		"https://rocket-league.com/*"
	  ],
	  "run_at": "document_end",
	  "css": [
		"styles/rlge_overrides.css"
	  ]
	},
	// Item prices (trades)
	{
	  "matches": [
		"https://rocket-league.com/trading",
		"https://rocket-league.com/player/*",
		"https://rocket-league.com/trades/*"
	  ],
	  "run_at": "document_end",
	  "js": [
		"scripts/utils.js",
		"scripts/item_prices/item_prices_utils.js",
		"scripts/item_prices/item_prices.js"
	  ],
	  "css": [
		"scripts/item_prices/item_prices.css"
	  ]
	},
	// Trade bumper
	{
	  "matches": [
		"https://rocket-league.com/trades/*"
	  ],
	  "run_at": "document_end",
	  "js": [
		"scripts/trade_bumper/trade_bumper.js"
	  ],
	  "css": [
		"scripts/trade_bumper/trade_bumper.css"
	  ]
	}
  ],
  "icons": {
	"16": "icons/icon-16.png",
	"32": "icons/icon-32.png",
	"48": "icons/icon-48.png",
	"64": "icons/icon-64.png",
	"128": "icons/icon-128.png",
	"256": "icons/icon-256.png"
  }
}